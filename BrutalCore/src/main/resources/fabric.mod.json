{
  "schemaVersion": 1,
  "id": "brutalcore",
  "version": "${version}",

  "name": "Brutal Core Mod",
  "description": "Core mod for Brutal Tech and Brutal Machines. Common entity library. Adds complexities to ore refining and gem processing.",
  "authors": [
    "David Boarman", "Ethan Boarman"
  ],
  "contact": {
  },

  "license": "CC0-1.0",
  "icon": "assets/brutalcore/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.brutalcore.mod.BrutalMod"
    ]
  },
  "mixins": [
    "brutalcore.mixins.json"
  ],

  "requires": {
    "fabricloader": ">=0.4.0",
    "fabric": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
